<p id="notice"><%= notice %></p>

<h1>Extractor Templates</h1>

<table>
  <thead>
    <tr>
      <th> sheetid</th>
      <th>Sheetname</th>
      <th>Primarykey</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extractor_templates.each do |extractor_template| %>
      <tr>
        <td><%= extractor_template.sheetID %></td>
        <td><%= extractor_template.SheetName %></td>
        <td><%= extractor_template.PrimaryKey %></td>
        <td><%= link_to 'Show', extractor_template %></td>
        <td><%= link_to 'Edit', edit_extractor_template_path(extractor_template) %></td>
        <td><%= link_to 'Destroy', extractor_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extractor Template', new_extractor_template_path %>
